{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "anime-api",
      "urls": [
        "https://api.jikan.moe/v3/**",
        "https://cdn.myanimelist.net/images/anime/**/**",
        "https://cdn.myanimelist.net/images/anime/**"
      ],
      "cacheConfig": {
        "maxSize": 20,
        "maxAge": "10h",
        "timeout": "1s",
        "strategy": "performance"
      }
    }
  ]
}
